# يمكنك تحديد نسخة iOS المستهدفة هنا
platform :ios, '13.0'

# ignore_all_warnings!  # يمكنك استخدام هذا السطر لتجاهل التحذيرات إذا كنت تواجه تحذيرات كثيرة

# إعداد المكتبات (Pods) الخاصة بمشروعك
target 'Runner' do
  use_frameworks!
  use_modular_headers!

  # تثبيت جميع مكتبات Flutter الأساسية للمشروع
  flutter_install_all_ios_pods(File.dirname(File.realpath(__FILE__)))
end

# إعداد مسار Flutter SDK في مشروعك
# هذا التحميل يتيح لك استيراد مكتبات Flutter
load File.join(File.dirname(__FILE__), '../.symlinks/flutter/ios/Flutter/podhelper.rb')
